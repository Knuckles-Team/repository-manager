{
  "mcpServers": {
    "repository-manager": {
      "url": "${MCP_URL:-http://localhost:8007/mcp}",
      "timeout": 300000
    },
    "mem0": {
      "command": "uv",
      "args": [
        "run",
        "mem0-mcp-server"
      ],
      "env": {
        "MEM0_API_KEY": "${MEM0_API_KEY}"
      },
      "timeout": 300000
    }
  }
}
